class User {
  init (id, name) {
    this.id = id;
    this.name = name;
  }
  getID() {
    return this.id;
  }
  getName () {
    return this.name;
  }
}

var mike = User(100, "mike");
var john = User(101, "john");

print mike.getID();
print mike.getName();

print john.getID();
print john.getName();
